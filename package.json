{
  "name": "concat7-blog-latest",
  "version": "1.0.0",
  "description": "ConCat7 Blog",
  "main": "index.js",
  "scripts": {
    "dev": "env-cmd -f ./config/dev.env nodemon src/index.js --ext js,graphql --exec babel-node",
    "migrate-dev": "env-cmd -f ./config/dev.env prisma migrate dev --preview-feature",
    "migrate-reset": "env-cmd -f ./config/dev.env prisma migrate reset --preview-feature",
    "migrate-resolve": "env-cmd -f ./config/dev.env prisma migrate resolve --preview-feature",
    "migrate-reset-test": "env-cmd -f ./config/test.env prisma migrate reset --preview-feature",
    "test": "env-cmd -f ./config/test.env jest --watchAll --detectOpenHandles --runInBand --testTimeout=12000"
  },
  "jest": {
    "globalSetup": "./tests/jest/globalSetup.js",
    "globalTeardown": "./tests/jest/globalTeardown.js"
  },
  "author": "Ikram Hasib",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@babel/polyfill": "^7.12.1",
    "@prisma/client": "^2.18.0",
    "bcryptjs": "^2.4.3",
    "cross-fetch": "^3.0.6",
    "env-cmd": "^10.1.0",
    "faker": "^5.4.0",
    "graphql": "^15.5.0",
    "graphql-yoga": "^1.18.3",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/node": "^7.12.16",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "@babel/register": "^7.13.8",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prisma": "^2.18.0"
  }
}
